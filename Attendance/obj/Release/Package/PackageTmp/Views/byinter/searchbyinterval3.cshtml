@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}


<script type="text/javascript">
    $(document).ready(function () {
        $("#frominputdate").datepicker({

            dateFormat: "yy-mm-dd"
        });
    });
</script>
<script type="text/javascript">
    $(document).ready(function () {
        $("#toinputdate").datepicker({

            dateFormat: "yy-mm-dd"
        });
    });
</script>
<script>
    $('#depname').change(function () {
        var depname = $('#depname').val();
        $("#empcode").val(depname);
    });

</script>
